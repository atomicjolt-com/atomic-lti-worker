{
  "private": true,
  "version": "1.1.0",
  "scripts": {
    "worker": "wrangler pages dev --compatibility-date=2023-06-28 ./public",
    "worker:remote": "wrangler pages dev --remote --compatibility-date=2023-06-28 ./public",
    "build": "npm run clean && node esbuild.mjs prod",
    "clean": "rm -rf public/dist",
    "dev:build": "npm run clean && node esbuild.mjs dev",
    "deploy": "npm run build && wrangler pages deploy ./public",
    "test": "vitest",
    "tail": "wrangler tail --format pretty"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^1.20231121.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "esbuild": "^0.19.7",
    "eslint": "^8.54.0",
    "eslint-config-typescript": "^3.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6",
    "vitest-environment-miniflare": "^2.14.0",
    "wrangler": "^3.17.0"
  },
  "dependencies": {
    "@atomicjolt/lti-server": "1.12.2",
    "@atomicjolt/lti-client": "^2.4.7",
    "@atomicjolt/lti-endpoints": "file:../atomicjolt-lti-endpoints",
    "@atomicjolt/lti-types": "^1.1.6",
    "hono": "^3.10.2",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "jose": "^5.1.1"
  }
}