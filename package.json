{
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "dev": "wrangler dev --live-reload",
    "remote": "wrangler pages dev --remote --compatibility-date=2025-03-04 ./public",
    "build": "npm run clean && node esbuild.mjs prod",
    "clean": "rm -rf public/dist",
    "dev:build": "npm run clean && node esbuild.mjs dev",
    "deploy": "npm run build && wrangler deploy ./public",
    "test": "vitest",
    "tail": "wrangler tail --format pretty",
    "types": "wrangler types --env-interface CloudflareBindings"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8",
    "@cloudflare/workers-types": "^4.20250317.0",
    "@typescript-eslint/eslint-plugin": "^8.26",
    "@typescript-eslint/parser": "^8.26",
    "esbuild": "^0.25",
    "eslint": "^9.22",
    "eslint-config-typescript": "^3.0",
    "prettier": "^3.5",
    "typescript": "^5.8",
    "vitest": "^3.0",
    "wrangler": "^4.1.0"
  },
  "dependencies": {
    "@atomicjolt/lti-client": "^2.6",
    "@atomicjolt/lti-endpoints": "3.0",
    "@atomicjolt/lti-server": "3.1",
    "@atomicjolt/lti-types": "^1.2",
    "hono": "^4.7",
    "i18next": "^24.2",
    "i18next-browser-languagedetector": "^8.0",
    "jose": "^6.0"
  }
}